      "email": "niklas@narhinen.net"
    },
    {
      "name": "Bart Van Houtte",
      "email": "bart.van.houtte@ading.be"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "lodash.once": "^4.1.1",
    "merge-class-names": "^1.0.0",
    "pdfjs-dist": "^1.9.537",
    "prop-types": ">=15.5",
    "react": ">=15.5",
    "react-dom": ">=15.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
